{
  "extends": "./tsconfig.paths.json",
  "include": [
    "server/**/*"
  ],
  "compilerOptions": {
    "target": "es6",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
  },
}
